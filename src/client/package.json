{
  "name": "client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,css,scss}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:analyze": "tsc -b && vite build --mode=analyze",
    "analyze": "npm run check:all && npm run build:analyze && npx vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:strict": "eslint . --max-warnings 0",
    "check:all": "npm run typecheck && npm run lint:strict",
    "preview": "vite preview",
    "perf:test": "node performance-test.js",
    "perf:baseline": "node performance-test.js && cp performance-results.json performance-baseline.json",
    "perf:compare": "node performance-test.js && echo 'Performance comparison completed. Check performance-report.html'",
    "perf:dev": "npm run dev & npm run perf:monitor",
    "perf:monitor": "echo 'Press Ctrl+Shift+P to toggle performance dashboard in browser'",
    "prepare": "cd ../.. && husky src/client/.husky"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fontsource/roboto": "^5.2.9",
    "@hookform/resolvers": "^5.2.2",
    "@microsoft/signalr": "^10.0.0",
    "@mui/icons-material": "^7.3.6",
    "@mui/lab": "^7.0.1-beta.20",
    "@mui/material": "^7.3.6",
    "@mui/styled-engine-sc": "^7.3.6",
    "@mui/x-date-pickers": "^8.21.0",
    "@reduxjs/toolkit": "^2.11.1",
    "@types/qrcode": "^1.5.6",
    "axios": "^1.13.2",
    "date-fns": "^4.1.0",
    "formik": "^2.4.9",
    "framer-motion": "^12.23.25",
    "qrcode": "^1.5.4",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-hook-form": "^7.68.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.10.1",
    "react-toastify": "^11.0.5",
    "simple-peer": "^9.11.1",
    "styled-components": "^6.1.19",
    "yup": "^1.7.1",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@stylistic/eslint-plugin": "^5.6.1",
    "@types/node": "^24.10.2",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.49.0",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "rollup-plugin-visualizer": "^6.0.5",
    "terser": "^5.44.1",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.49.0",
    "vite": "^7.2.7"
  }
}
